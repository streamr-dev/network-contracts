{
  "name": "smartcontracts",
  "version": "0.0.4",
  "description": "Smart contracts for Streamr Network",
  "private": true,
  "scripts": {
    "build": "hardhat compile",
    "clean": "rm -rf artifacts cache coverage typechain deployments .openzeppelin",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "lint": "eslint test/*.* && solhint contracts/*/*.sol",
    "deploy": "hardhat run --network hardhat scripts/deploy.ts",
    "deployLocalWithExport": "hardhat --network localsidechain deploy",
    "deployProxy": "hardhat run --network hardhat scripts/deployWithProxy.ts",
    "upgradeImpl": "hardhat run --network hardhat scripts/upgradeProxy.ts",
    "localDeploy": "hardhat run --network localsidechain scripts/deploy.ts",
    "localDeployProxy": "hardhat run --network localsidechain scripts/deployWithProxy.ts",
    "localUpgradeImpl": "hardhat run --network localsidechain scripts/upgradeProxy.ts",
    "interactProd": "hardhat run --network polygonMainnet scripts/deployToLivenet/2_interactWithContracts.ts",
    "interact": "hardhat run --network localsidechain scripts/deployToLivenet/2_interactWithContracts.ts",
    "interact1": "hardhat run --network polygonTestMumbai1 scripts/deployToLivenet/2_interactWithContracts.ts",
    "interact2": "hardhat run --network localhost scripts/deployToLivenet/2_interactWithContracts.ts",
    "deploylive2": "hardhat run --network localhost scripts/deployToLivenet/1_deployContracts.ts",
    "deploylive1": "hardhat run --network polygonTestMumbai1 scripts/deployToLivenet/1_deployContracts.ts",
    "deploylive": "hardhat run --network localsidechain scripts/deployToLivenet/1_deployContracts.ts",
    "deployliveProd": "hardhat run --network polygonMainnet scripts/deployToLivenet/1_deployContracts.ts",
    "migrateStreams": "hardhat run --network localsidechain scripts/migration/migrateStreams.ts",
    "migrateStreams2": "hardhat run --network localsidechain scripts/migration/migrateStreamsHighGas.ts",
    "cleanupStreams": "hardhat run --network localsidechain scripts/migration/cleanupStreams.ts",
    "sstest": "hardhat run --network localsidechain scripts/registerStorageNode.ts"
  },
