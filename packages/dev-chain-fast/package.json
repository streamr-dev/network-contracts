{
  "name": "@streamr/dev-chain-fast",
  "version": "0.1.0",
  "description": "Locally run Ethereum client for developing the Streamr services",
  "private": true,
  "scripts": {
    "buildLocalArch": "./prepare.sh && docker build -t streamr/dev-chain-fast:dev .",
    "buildMultiArchAndPush": "./prepare.sh && docker buildx build --platform linux/amd64,linux/arm64 -t streamr/dev-chain-fast:dev --push .",
    "builda": "docker build -t streamr/dev-chain-fast .",
    "buildb": "./prepare.sh",
    "deploy": "tsx deploy.ts",
    "clean": "rm -fr node_modules"
  },
  "author": "Streamr Network AG",
  "license": "ISC",
  "dependencies": {
    "@streamr/hub-contracts": "^1.1.2",
    "@streamr/network-contracts": "^4.3.3",
    "@dataunions/contracts": "^3.0.8",
    "hardhat": "^2.17.1",
    "tsx": "^3.12.7"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
