{
  "name": "streamr-config",
  "version": "0.0.1",
  "description": "Zero dependency package that contains Streamr Network Contract addresses",
  "main": "dist/index.js",
  "types": "dist/main.d.ts",
  "directories": {
    "lib": "./dist",
    "src": "./src",
    "test": "./src"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/chai": "4.2.22",
    "@types/mocha": "9.0.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "chai": "4.3.4",
    "eslint": "8.2.0",
    "eslint-plugin-jsdoc": "37.0.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "mocha": "9.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.2",
    "typescript-eslint-parser": "22.0.0"
  },
  "scripts": {
    "build": "npm run prepare",
    "prepare": "tsc --project tsconfig.json && cp src/main.d.ts dist/src",
    "test": "mocha --config mocharc.json",
    "lint": "eslint --config .eslintrc.js --ext .ts src test",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamr-dev/network-contracts.git"
  },
  "keywords": [
    "streamr",
    "ethereum",
    "contract",
    "address"
  ],
  "author": "Kare Nuorteva <kare.nuorteva@streamr.network>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streamr-dev/network-contracts/issues"
  },
  "homepage": "https://github.com/streamr-dev/network-contracts#readme"
}
